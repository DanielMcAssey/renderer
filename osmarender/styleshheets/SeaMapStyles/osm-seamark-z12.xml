<?xml-stylesheet type="text/xsl" href="osmarender.xsl"?>


<!-- OpenSeaMap Rules File for zoom level 12 -->

<rules data="data.osm" scale="1" symbolScale="0.4" minimumMapWidth="0.5" minimumMapHeight="0.5" showScale="no" showGrid="no"
	showBorder="no" showLicense="no" showZoomControls="no" symbolsDir="../../../OpenSeaMap-styles/SeaMapSymbols" meter2pixel="0.1375">

	<!-- Lateral Buoys -->

	<rule e="node" k="seamark:buoy_lateral:category" v="*">

		<!-- Port Buoys -->

		<rule e="node" k="seamark:buoy_lateral:category" v="port">

			<!-- IALA-A -->

			<rule e="node" k="seamark:buoy_lateral:colour" v="red">
				<rule e="node" k="seamark:buoy_lateral:shape" v="*">
					<rule e="node" k="seamark:buoy_lateral:shape" v="pillar">
						<symbol ref="Lateral_Pillar_Red" position="center" />
					</rule>
					<rule e="node" k="seamark:buoy_lateral:shape" v="spar">
						<symbol ref="Lateral_Spar_Red" position="center" />
					</rule>
					<rule e="node" k="seamark:buoy_lateral:shape" v="can">
						<symbol ref="Lateral_Can_Red" position="center" />
					</rule>
				</rule>
				<else>
					<symbol ref="Lateral_Pillar_Red" position="center" />
				</else>
			</rule>

			<!-- IALA-B -->

			<rule e="node" k="seamark:buoy_lateral:colour" v="green">
				<rule e="node" k="seamark:buoy_lateral:shape" v="*">
					<rule e="node" k="seamark:buoy_lateral:shape" v="pillar">
						<symbol ref="Lateral_Pillar_Green" position="center" />
					</rule>
					<rule e="node" k="seamark:buoy_lateral:shape" v="spar">
						<symbol ref="Lateral_Spar_Green" position="center" />
					</rule>
					<rule e="node" k="seamark:buoy_lateral:shape" v="can">
						<symbol ref="Lateral_Can_Green" position="center" />
					</rule>
				</rule>
				<else>
					<symbol ref="Lateral_Pillar_Green" position="center" />
				</else>
			</rule>
		</rule>

		<!-- Starboard Buoys -->

		<rule e="node" k="seamark:buoy_lateral:category" v="starboard">

			<!-- IALA-A -->

			<rule e="node" k="seamark:buoy_lateral:colour" v="green">
				<rule e="node" k="seamark:buoy_lateral:shape" v="*">
					<rule e="node" k="seamark:buoy_lateral:shape" v="pillar">
						<symbol ref="Lateral_Pillar_Green" position="center" />
					</rule>
					<rule e="node" k="seamark:buoy_lateral:shape" v="spar">
						<symbol ref="Lateral_Spar_Green" position="center" />
					</rule>
					<rule e="node" k="seamark:buoy_lateral:shape" v="conical">
						<symbol ref="Lateral_Cone_Green" position="center" />
					</rule>
				</rule>
				<else>
					<symbol ref="Lateral_Pillar_Green" position="center" />
				</else>
			</rule>

			<!-- IALA-B -->

			<rule e="node" k="seamark:buoy_lateral:colour" v="red">
				<rule e="node" k="seamark:buoy_lateral:shape" v="*">
					<rule e="node" k="seamark:buoy_lateral:shape" v="pillar">
						<symbol ref="Lateral_Pillar_Red" position="center" />
					</rule>
					<rule e="node" k="seamark:buoy_lateral:shape" v="spar">
						<symbol ref="Lateral_Spar_Red" position="center" />
					</rule>
					<rule e="node" k="seamark:buoy_lateral:shape" v="conical">
						<symbol ref="Lateral_Cone_Red" position="center" />
					</rule>
					<else>
						<symbol ref="Lateral_Pillar_Red" position="center" />
					</else>
				</rule>
			</rule>
		</rule>

		<!-- Preferred Channel Port Buoys -->

		<rule e="node" k="seamark:buoy_lateral:category" v="preferred_channel_port">

			<!-- IALA-A -->

			<rule e="node" k="seamark:buoy_lateral:colour" v="red;green;red">
				<rule e="node" k="seamark:buoy_lateral:shape" v="*">
					<rule e="node" k="seamark:buoy_lateral:shape" v="pillar">
						<symbol ref="Lateral_Pillar_Red_Green_Red" position="center" />
					</rule>
					<rule e="node" k="seamark:buoy_lateral:shape" v="spar">
						<symbol ref="Lateral_Spar_Red_Green_Red" position="center" />
					</rule>
					<rule e="node" k="seamark:buoy_lateral:shape" v="can">
						<symbol ref="Lateral_Can_Red_Green_Red" position="center" />
					</rule>
				</rule>
				<else>
					<symbol ref="Lateral_Pillar_Red_Green_Red" position="center" />
				</else>
			</rule>

			<!-- IALA-B -->

			<rule e="node" k="seamark:buoy_lateral:colour" v="green;red;green">
				<rule e="node" k="seamark:buoy_lateral:shape" v="*">
					<rule e="node" k="seamark:buoy_lateral:shape" v="pillar">
						<symbol ref="Lateral_Pillar_Green_Red_Green" position="center" />
					</rule>
					<rule e="node" k="seamark:buoy_lateral:shape" v="spar">
						<symbol ref="Lateral_Spar_Green_Red_Green" position="center" />
					</rule>
					<rule e="node" k="seamark:buoy_lateral:shape" v="can">
						<symbol ref="Lateral_Can_Green_Red_Green" position="center" />
					</rule>
				</rule>
				<else>
					<symbol ref="Lateral_Pillar_Green_Red_Green" position="center" />
				</else>
			</rule>
		</rule>

		<!-- Preferred Channel Starboard Buoys -->

		<rule e="node" k="seamark:buoy_lateral:category" v="preferred_channel_starboard">

			<!-- IALA-A -->

			<rule e="node" k="seamark:buoy_lateral:colour" v="green;red;green">
				<rule e="node" k="seamark:buoy_lateral:shape" v="*">
					<rule e="node" k="seamark:buoy_lateral:shape" v="pillar">
						<symbol ref="Lateral_Pillar_Green_Red_Green" position="center" />
					</rule>
					<rule e="node" k="seamark:buoy_lateral:shape" v="spar">
						<symbol ref="Lateral_Spar_Green_Red_Green" position="center" />
					</rule>
					<rule e="node" k="seamark:buoy_lateral:shape" v="conical">
						<symbol ref="Lateral_Cone_Green_Red_Green" position="center" />
					</rule>
				</rule>
				<else>
					<symbol ref="Lateral_Pillar_Green_Red_Green" position="center" />
				</else>
			</rule>

			<!-- IALA-B -->

			<rule e="node" k="seamark:buoy_lateral:colour" v="red;green;red">
				<rule e="node" k="seamark:buoy_lateral:shape" v="*">
					<rule e="node" k="seamark:buoy_lateral:shape" v="pillar">
						<symbol ref="Lateral_Pillar_Red_Green_Red" position="center" />
					</rule>
					<rule e="node" k="seamark:buoy_lateral:shape" v="spar">
						<symbol ref="Lateral_Spar_Red_Green_Red" position="center" />
					</rule>
					<rule e="node" k="seamark:buoy_lateral:shape" v="conical">
						<symbol ref="Lateral_Cone_Red_Green_Red" position="center" />
					</rule>
				</rule>
				<else>
					<symbol ref="Lateral_Pillar_Red_Green_Red" position="center" />
				</else>
			</rule>
		</rule>
	</rule>

	<!-- Lateral Beacons -->

	<rule e="node" k="seamark:beacon_lateral:category" v="*">

		<!-- Port Beacons -->

		<rule e="node" k="seamark:beacon_lateral:category" v="port">

			<!-- IALA-A -->

			<rule e="node" k="seamark:beacon_lateral:colour" v="red">
				<rule e="node" k="seamark:beacon_lateral:shape" v="tower">
					<symbol ref="Lateral_Tower_Red" position="center" />
				</rule>
				<else>
					<symbol ref="Lateral_Beacon_Red" position="center" />
				</else>
			</rule>

			<!-- IALA-B -->

			<rule e="node" k="seamark:beacon_lateral:colour" v="green">
				<rule e="node" k="seamark:beacon_lateral:shape" v="tower">
					<symbol ref="Lateral_Tower_Green" position="center" />
				</rule>
				<else>
					<symbol ref="Lateral_Beacon_Green" position="center" />
				</else>
			</rule>
		</rule>

		<!-- Starboard Beacons -->

		<rule e="node" k="seamark:beacon_lateral:category" v="starboard">

			<!-- IALA-A -->

			<rule e="node" k="seamark:beacon_lateral:colour" v="green">
				<rule e="node" k="seamark:beacon_lateral:shape" v="tower">
					<symbol ref="Lateral_Tower_Green" position="center" />
				</rule>
				<else>
					<symbol ref="Lateral_Beacon_Green" position="center" />
				</else>
			</rule>

			<!-- IALA-B -->

			<rule e="node" k="seamark:beacon_lateral:colour" v="red">
				<rule e="node" k="seamark:beacon_lateral:shape" v="tower">
					<symbol ref="Lateral_Tower_Red" position="center" />
				</rule>
				<else>
					<symbol ref="Lateral_Beacon_Red" position="center" />
				</else>
			</rule>
		</rule>

		<!-- Referred Channel Port Beacons -->

		<rule e="node" k="seamark:beacon_lateral:category" v="preferred_channel_port">

			<!-- IALA-A -->

			<rule e="node" k="seamark:beacon_lateral:colour" v="red;green;red">
				<rule e="node" k="seamark:beacon_lateral:shape" v="tower">
					<symbol ref="Lateral_Tower_Red_Green_Red" position="center" />
				</rule>
				<else>
					<symbol ref="Lateral_Beacon_Red_Green_Red" position="center" />
				</else>
			</rule>

			<!-- IALA-B -->

			<rule e="node" k="seamark:beacon_lateral:colour" v="green;red;green">
				<rule e="node" k="seamark:beacon_lateral:shape" v="tower">
					<symbol ref="Lateral_Tower_Green_Red_Green" position="center" />
				</rule>
				<else>
					<symbol ref="Lateral_Beacon_Green_Red_Green" position="center" />
				</else>
			</rule>
		</rule>

		<!-- Preferred Channel Starboard Beacons -->

		<rule e="node" k="seamark:beacon_lateral:category" v="preferred_channel_starboard">

			<!-- IALA-A -->

			<rule e="node" k="seamark:beacon_lateral:colour" v="green;red;green">
				<rule e="node" k="seamark:beacon_lateral:shape" v="tower">
					<symbol ref="Lateral_Tower_Green_Red_Green" position="center" />
				</rule>
				<else>
					<symbol ref="Lateral_Beacon_Green_Red_Green" position="center" />
				</else>
			</rule>

			<!-- IALA-B -->

			<rule e="node" k="seamark:beacon_lateral:colour" v="red;green;red">
				<rule e="node" k="seamark:beacon_lateral:shape" v="tower">
					<symbol ref="Lateral_Tower_Red_Green_Red" position="center" />
				</rule>
				<else>
					<symbol ref="Lateral_Beacon_Red_Green_Red" position="center" />
				</else>
			</rule>
		</rule>
	</rule>

	<!-- Lateral Light Floats -->

	<!-- Red Float -->

	<rule e="node" k="seamark:light_float:colour" v="red">
		<symbol ref="Lateral_Float_Red" position="center" />
	</rule>

	<!-- Green Float -->

	<rule e="node" k="seamark:light_float:colour" v="green">
		<symbol ref="Lateral_Float_Green" position="center" />
	</rule>

	<!-- Red-Green-Red Float -->

	<rule e="node" k="seamark:light_float:colour" v="red;green;red">
		<symbol ref="Lateral_Float_Red_Green_Red" position="center" />
	</rule>

	<!-- Green-Red-Green Float -->

	<rule e="node" k="seamark:light_float:colour" v="green;red;green">
		<symbol ref="Lateral_Float_Green_Red_Green" position="center" />
	</rule>

	<!-- Cardinal-Marks -->

	<!-- Buoy -->

	<rule e="node" k="seamark:buoy_cardinal:category" v="*">
		<rule e="node" k="seamark:buoy_cardinal:category" v="north">
			<rule e="node" k="seamark:buoy_cardinal:shape" v="*">
				<rule e="node" k="seamark:buoy_cardinal:shape" v="pillar">
					<symbol ref="Cardinal_Pillar_North" position="center" />
				</rule>
				<rule e="node" k="seamark:buoy_cardinal:shape" v="spar">
					<symbol ref="Cardinal_Spar_North" position="center" />
				</rule>
			</rule>
		</rule>
		<rule e="node" k="seamark:buoy_cardinal:category" v="east">
			<rule e="node" k="seamark:buoy_cardinal:shape" v="*">
				<rule e="node" k="seamark:buoy_cardinal:shape" v="pillar">
					<symbol ref="Cardinal_Pillar_East" position="center" />
				</rule>
				<rule e="node" k="seamark:buoy_cardinal:shape" v="spar">
					<symbol ref="Cardinal_Spar_East" position="center" />
				</rule>
			</rule>
		</rule>
		<rule e="node" k="seamark:buoy_cardinal:category" v="south">
			<rule e="node" k="seamark:buoy_cardinal:shape" v="*">
				<rule e="node" k="seamark:buoy_cardinal:shape" v="pillar">
					<symbol ref="Cardinal_Pillar_South" position="center" />
				</rule>
				<rule e="node" k="seamark:buoy_cardinal:shape" v="spar">
					<symbol ref="Cardinal_Spar_South" position="center" />
				</rule>
			</rule>
		</rule>
		<rule e="node" k="seamark:buoy_cardinal:category" v="west">
			<rule e="node" k="seamark:buoy_cardinal:shape" v="*">
				<rule e="node" k="seamark:buoy_cardinal:shape" v="pillar">
					<symbol ref="Cardinal_Pillar_West" position="center" />
				</rule>
				<rule e="node" k="seamark:buoy_cardinal:shape" v="spar">
					<symbol ref="Cardinal_Spar_West" position="center" />
				</rule>
			</rule>
		</rule>
	</rule>

	<!-- Beacon -->

	<rule e="node" k="seamark:beacon_cardinal:category" v="*">
		<rule e="node" k="seamark:beacon_cardinal:category" v="north">
			<rule e="node" k="seamark:beacon_cardinal:shape" v="*">
				<rule e="node" k="seamark:beacon_cardinal:shape" v="tower">
					<symbol ref="Cardinal_Tower_North" position="center" />
				</rule>
				<else>
					<symbol ref="Cardinal_Beacon_North" position="center" />
				</else>
			</rule>
			<else>
				<symbol ref="Cardinal_Beacon_North" position="center" />
			</else>
		</rule>
		<rule e="node" k="seamark:beacon_cardinal:category" v="east">
			<rule e="node" k="seamark:beacon_cardinal:shape" v="*">
				<rule e="node" k="seamark:beacon_cardinal:shape" v="tower">
					<symbol ref="Cardinal_Tower_East" position="center" />
				</rule>
				<else>
					<symbol ref="Cardinal_Beacon_East" position="center" />
				</else>
			</rule>
			<else>
				<symbol ref="Cardinal_Beacon_North" position="center" />
			</else>
		</rule>
		<rule e="node" k="seamark:beacon_cardinal:category" v="south">
			<rule e="node" k="seamark:beacon_cardinal:shape" v="*">
				<rule e="node" k="seamark:beacon_cardinal:shape" v="tower">
					<symbol ref="Cardinal_Tower_South" position="center" />
				</rule>
				<else>
					<symbol ref="Cardinal_Beacon_South" position="center" />
				</else>
			</rule>
			<else>
				<symbol ref="Cardinal_Beacon_North" position="center" />
			</else>
		</rule>
		<rule e="node" k="seamark:beacon_cardinal:category" v="west">
			<rule e="node" k="seamark:beacon_cardinal:shape" v="*">
				<rule e="node" k="seamark:beacon_cardinal:shape" v="tower">
					<symbol ref="Cardinal_Tower_West" position="center" />
				</rule>
				<else>
					<symbol ref="Cardinal_Beacon_West" position="center" />
				</else>
			</rule>
			<else>
				<symbol ref="Cardinal_Beacon_North" position="center" />
			</else>
		</rule>
	</rule>

	<!-- Cardinal Light Floats -->

	<rule e="node" k="seamark:light_float:colour" v="black;yellow">
		<symbol ref="Cardinal_Float_North" position="center" />
	</rule>
	<rule e="node" k="seamark:light_float:colour" v="black;yellow;black">
		<symbol ref="Cardinal_Float_East" position="center" />
	</rule>
	<rule e="node" k="seamark:light_float:colour" v="yellow;black">
		<symbol ref="Cardinal_Float_South" position="center" />
	</rule>
	<rule e="node" k="seamark:light_float:colour" v="yellow;black;yellow">
		<symbol ref="Cardinal_Float_West" position="center" />
	</rule>

	<!--Safe water Buoys -->

	<rule e="node" k="seamark:type" v="buoy_safe_water">
		<rule e="node" k="seamark:buoy_safe_water:shape" v="*">
			<rule e="node" k="seamark:buoy_safe_water:shape" v="pillar">
				<symbol ref="Safe_Water_Pillar" position="center" />
			</rule>
			<rule e="node" k="seamark:buoy_safe_water:shape" v="spar">
				<symbol ref="Safe_Water_Spar" position="center" />
			</rule>
			<rule e="node" k="seamark:buoy_safe_water:shape" v="sphere">
				<symbol ref="Safe_Water_Sphere" position="center" />
			</rule>
		</rule>
		<else>
			<symbol ref="Safe_Water_Pillar" position="center" />
		</else>
	</rule>

	<!-- Safe Water Beacon -->

	<rule e="node" k="seamark:beacon_safe_water:colour" v="red;white">
		<symbol ref="Safe_Water_Beacon" position="center" />
	</rule>

	<!-- Safe Water Float -->

	<rule e="node" k="seamark:light_float:colour" v="red;white">
		<symbol ref="Safe_Water_Float" position="center" />
	</rule>

	<!--SpecialPurpose Buoys -->

	<rule e="node" k="seamark:type" v="buoy_special_purpose">
		<rule e="node" k="seamark:buoy_special_purpose:shape" v="*">
			<rule e="node" k="seamark:buoy_special_purpose:shape" v="pillar">
				<symbol ref="Special_Purpose_Pillar" position="center" />
			</rule>
			<rule e="node" k="seamark:buoy_special_purpose:shape" v="spar">
				<symbol ref="Special_Purpose_Spar" position="center" />
			</rule>
			<rule e="node" k="seamark:buoy_special_purpose:shape" v="can">
				<symbol ref="Special_Purpose_Can" position="center" />
			</rule>
			<rule e="node" k="seamark:buoy_special_purpose:shape" v="conical">
				<symbol ref="Special_Purpose_Cone" position="center" />
			</rule>
			<rule e="node" k="seamark:buoy_special_purpose:shape" v="sphere">
				<symbol ref="Special_Purpose_Sphere" position="center" />
			</rule>
			<rule e="node" k="seamark:buoy_special_purpose:shape" v="barrel">
				<symbol ref="Special_Purpose_Barrel" position="center" />
			</rule>
		</rule>
	</rule>

	<!-- Special Purpose Floats -->

	<rule e="node" k="seamark:light_float:colour" v="yellow">
		<symbol ref="Special_Purpose_Float" position="center" />
	</rule>

	<!-- Special Purpose Beacons -->

	<rule e="node" k="seamark:type" v="beacon_special_purpose">
		<rule e="node" k="seamark:beacon_special_purpose:shape" v="tower">
			<symbol ref="Special_Purpose_Tower" position="center" />
		</rule>
		<else>
			<symbol ref="Special_Purpose_Beacon" position="center" />
		</else>
	</rule>

	<!-- Isolated Danger Buoys -->

	<rule e="node" k="seamark:buoy_isolated_danger:shape" v="*">
		<rule e="node" k="seamark:buoy_isolated_danger:shape" v="pillar">
			<symbol ref="Cardinal_Pillar_Single" position="center" />
		</rule>
		<rule e="node" k="seamark:buoy_isolated_danger:shape" v="spar">
			<symbol ref="Cardinal_Spar_Single" position="center" />
		</rule>
	</rule>

	<!-- Isolated Danger Beacons -->

	<rule e="node" k="seamark:type" v="beacon_isolated_danger">
		<rule e="node" k="seamark:beacon_isolated_danger:shape" v="tower">
			<symbol ref="Cardinal_Tower_Single" position="center" />
		</rule>
		<else>
			<symbol ref="Cardinal_Beacon_Single" position="center" />
		</else>
	</rule>

	<!-- Isolated Danger Light Float -->

	<rule e="node" k="seamark:light_float:colour" v="black;red;black">
		<symbol ref="Cardinal_Float_Single" position="center" />
	</rule>

	<!-- Topmarks -->

	<rule e="node" k="seamark:topmark:shape" v="cylinder">
		<rule e="node" k="seamark:buoy_lateral:shape|seamark:buoy_special_purpose:shape" v="pillar|spar">
			<rule e="node" k="seamark:topmark:colour" v="red">
				<symbol ref="Top_Can_Red_Buoy" position="center" />
			</rule>
			<else>
				<rule e="node" k="seamark:topmark:colour" v="green">
					<symbol ref="Top_Can_Green_Buoy" position="center" />
				</rule>
				<else>
					<rule e="node" k="seamark:topmark:colour" v="yellow">
						<symbol ref="Top_Can_Yellow_Buoy" position="center" />
					</rule>
					<else>
						<symbol ref="Top_Can_Buoy" position="center" />
					</else>
				</else>
			</else>
		</rule>
		<rule e="node" k="seamark:buoy_lateral:shape|seamark:buoy_special_purpose:shape" v="can">
			<rule e="node" k="seamark:topmark:colour" v="red">
				<symbol ref="Top_Can_Red_Buoy_Small" position="center" />
			</rule>
			<else>
				<rule e="node" k="seamark:topmark:colour" v="green">
					<symbol ref="Top_Can_Green_Buoy_Small" position="center" />
				</rule>
				<else>
					<rule e="node" k="seamark:topmark:colour" v="yellow">
						<symbol ref="Top_Can_Yellow_Buoy_Small" position="center" />
