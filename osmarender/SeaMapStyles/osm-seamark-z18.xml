	<!-- Light characters -->

	<rule e="node" k="seamark:light:character" v="*">
		<rule e="node" k="seamark:light:group" v="*">
			<rule e="node" k="seamark:light:period" v="*">
				<rule e="node" k="seamark:light:colour" v="red">
					<text class="light-character" dx="2" dy="0"><tag k="seamark:light:character" />(<tag k="seamark:light:group"/>).R.<tag k="seamark:light:period"/>s</text>
				</rule>
				<rule e="node" k="seamark:light:colour" v="green">
					<text class="light-character" dx="2" dy="0"><tag k="seamark:light:character" />(<tag k="seamark:light:group"/>).G.<tag k="seamark:light:period"/>s</text>
				</rule>
				<rule e="node" k="seamark:light:colour" v="white">
					<rule e="node" k="seamark:light:character" v="Q+LFl|VQ+LFl|UQ+LFl">
						<rule e="node" k="seamark:light:character" v="Q+LFl">
							<text class="light-character" dx="2" dy="0">Q(<tag k="seamark:light:group"/>)+LFl.<tag k="seamark:light:period"/>s</text>
						</rule>
						<rule e="node" k="seamark:light:character" v="VQ+LFl">
							<text class="light-character" dx="2" dy="0">VQ(<tag k="seamark:light:group"/>)+LFl.<tag k="seamark:light:period"/>s</text>
						</rule>
						<rule e="node" k="seamark:light:character" v="UQ+LFl">
							<text class="light-character" dx="2" dy="0">UQ(<tag k="seamark:light:group"/>)+LFl.<tag k="seamark:light:period"/>s</text>
						</rule>
					</rule>
					<else>
						<text class="light-character" dx="2" dy="0"><tag k="seamark:light:character"/>(<tag k="seamark:light:group"/>)<tag k="seamark:light:period"/>s</text>
					</else>
				</rule>
			</rule>
			<else>
				<rule e="node" k="seamark:light:colour" v="red">
					<text class="light-character" dx="2" dy="0"><tag k="seamark:light:character"/>(<tag k="seamark:light:group"/>)R</text>
				</rule>
				<rule e="node" k="seamark:light:colour" v="green">
					<text class="light-character" dx="2" dy="0"><tag k="seamark:light:character"/>(<tag k="seamark:light:group"/>)G</text>
				</rule>
				<rule e="node" k="seamark:light:colour" v="white">
					<rule e="node" k="seamark:light:character" v="Q+LFl|VQ+LFl|UQ+LFl">
						<rule e="node" k="seamark:light:character" v="Q+LFl">
							<text class="light-character" dx="2" dy="0">Q(<tag k="seamark:light:group"/>)+LFl</text>
						</rule>
						<rule e="node" k="seamark:light:character" v="VQ+LFl">
							<text class="light-character" dx="2" dy="0">VQ(<tag k="seamark:light:group"/>)+LFl</text>
						</rule>
						<rule e="node" k="seamark:light:character" v="UQ+LFl">
							<text class="light-character" dx="2" dy="0">UQ(<tag k="seamark:light:group"/>)+LFl</text>
						</rule>
					</rule>
					<else>
						<text class="light-character" dx="2" dy="0"><tag k="seamark:light:character"/>(<tag k="seamark:light:group"/>)</text>
					</else>
				</rule>
			</else>
		</rule>
