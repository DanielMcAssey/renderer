  <!-- Signal Stations -->
  
  <rule e="node" k="seamark:type" v="signal_station_traffic">
    <symbol ref="Signal_Station" position="center" />
    <rule e="node" k="seamark:signal_station_traffic:category" v="*">
      <text class="light-character" dx="1" dy="0">SS(<tag k="seamark:signal_station_traffic:category"/>)</text>
    </rule>
    <else>
      <text class="light-character" dx="1" dy="0">SS</text>
    </else>
    <rule e="node" k="seamark:signal_station_traffic:channel" v="*">
      <text class="light-character" dx="1" dy="2">Ch.<tag k="seamark:signal_station_traffic:channel"/></text>
    </rule>
  </rule>
  
  <rule e="node" k="seamark:type" v="signal_station_warning">
    <symbol ref="Signal_Station" position="center" />
    <rule e="node" k="seamark:signal_station_warning:category" v="*">
      <text class="light-character" dx="1" dy="0">SS(<tag k="seamark:signal_station_warning:category"/>)</text>
    </rule>
    <else>
      <text class="light-character" dx="1" dy="0">SS</text>
    </else>
    <rule e="node" k="seamark:signal_station_warning:channel" v="*">
      <text class="light-character" dx="1" dy="2">Ch.<tag k="seamark:signal_station_warning:channel"/></text>
    </rule>
  </rule>
  
	<!-- Names -->

	<rule e="node" k="seamark:name" v="*">
		<text k="seamark:name" class="buoy-name" dx="2" dy="-2" />
	</rule>
	  
	<!-- Leading & Transit Lines -->

	<rule e="way" k="seamark:leading_line" v="*">
		<line class="leading-line" />
		<pathText k="seamark:leading_line" class="line-character" startOffset="50%" dy="-1.0" />
	</rule>

	<rule e="way" k="seamark:transit_line" v="*">
		<line class="transit-line" />
		<pathText k="seamark:transit_line" class="line-character" startOffset="50%" dy="-1.0" />
	</rule>


	<!-- Shoreline

	<rule e="way" k="natural" v="coastline">
		<line class="coast" layer="-4" />
	</rule>

	<rule e="way" k="natural" v="water">
		<line class="coast" layer="-4" />
	</rule>

	<rule e="way" k="waterway" v="riverbank">
		<line class="coast" layer="-4" />
	</rule>
-->

	<!--Harbours -->

	<!--<rule e="node" k="leisure" v="marina"> <symbol ref="Marina" position="center"/> <text k ='name' class ='harbour' dx='5'/> </rule> -->

	<!--<rule e="way" k="man_made" v="pier">
