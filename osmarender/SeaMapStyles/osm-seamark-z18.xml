		<pathText k="name" class="harbour" text-anchor="left" dy="0.33px" dx="10px" startOffset="30%" />
	</rule>
-->
	
	<rule e="way" k="man_made" v="breakwater">
		<line class="breakwater-base" layer="-5" />
		<line class="breakwater-overlay" layer="-5" />
	</rule>

	<rule e="node" k="leisure" v="slipway">
		<symbol ref="Slipway" position="center" />
	</rule>

	<rule e="node" k="man_made" v="crane">
		<symbol ref="crane" position="center" />
	</rule>

	<rule e="node" k="radio" v="coast_radio_station">
		<symbol ref="radio" position="center" />
		<text k='name' class='buoy-name' dy='-2' dx='4' />
	</rule>

	<!--harbour keys -->
	<rule e="node" k="harbour" v="harbour_master">
		<symbol ref="harbour_master" position="center" />
	</rule>
   
	<rule e="node" k="harbour|waterway" v="fuel">
		<symbol ref="Fuel" position="center" />
	</rule>

	<rule e="node" k="harbour" v="dolphin">
	   <circle r="0.25" class="harbour_dolphin" />
	</rule>
   
	<!--Waterway keys -->
	<rule e="node" k="waterway" v="no_entry">
		<symbol ref="No_Entry" position="center" />
	</rule>

	<rule e="node" k="waterway" v="turning_point">
		<symbol ref="Turning_Point" position="center" />
	</rule>

	<rule e="node" k="waterway:maxheight" v="*">
		<symbol ref="Max_Height" position="center" />
		<text class="waterway_sign" dx="1.5" dy="0.5">
			<tag k="waterway:maxheight" />m
		</text>
	</rule>
   
	<rule e="node" k="waterway:mindepth" v="*">
		<symbol ref="Min_Depth" position="center" />
		<text class="waterway_sign" dx="1.5" dy="0.5">
			<tag k="waterway:mindepth" />m
		</text>
	</rule>

	<rule e="node" k="waterway" v="lock_gate">
		<symbol ref="Lock_Gate_Single" position="center" />
	</rule>

	<rule e="node" k="waterway" v="weir">
		<symbol ref="Weir_Big" position="center" />
	</rule>

