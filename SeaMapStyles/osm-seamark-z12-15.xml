					<rule e="node" k="seamark:light:period" v="*">
						<rule e="node" k="seamark:light:colour" v="red">
							<text class="light-character" dx="1.9" dy="0"><tag k="seamark:light:multiple"/> <tag k="seamark:light:character"/>.R.<tag k="seamark:light:period"/>s</text>
						</rule>
						<rule e="node" k="seamark:light:colour" v="green">
							<text class="light-character" dx="1.9" dy="0"><tag k="seamark:light:multiple"/> <tag k="seamark:light:character"/>.G.<tag k="seamark:light:period"/>s</text>
						</rule>
						<rule e="node" k="seamark:light:colour" v="white">
							<text class="light-character" dx="1.9" dy="0"><tag k="seamark:light:multiple"/> <tag k="seamark:light:character"/>.<tag k="seamark:light:period"/>s</text>
						</rule>
					</rule>
					<else>
						<rule e="node" k="seamark:light:colour" v="red">
							<text class="light-character" dx="1.9" dy="0"><tag k="seamark:light:multiple"/> <tag k="seamark:light:character"/>.R</text>
						</rule>
						<rule e="node" k="seamark:light:colour" v="green">
							<text class="light-character" dx="1.9" dy="0"><tag k="seamark:light:multiple"/> <tag k="seamark:light:character"/>.G</text>
						</rule>
						<rule e="node" k="seamark:light:colour" v="white">
							<text class="light-character" dx="1.9" dy="0"><tag k="seamark:light:multiple"/> <tag k="seamark:light:character"/></text>
						</rule>
					</else>
				</rule>
				<else>
					<rule e="node" k="seamark:light:period" v="*">
						<rule e="node" k="seamark:light:colour" v="red">
							<text class="light-character" dx="1.9" dy="0"><tag k="seamark:light:character"/>.R.<tag k="seamark:light:period"/>s</text>
						</rule>
						<rule e="node" k="seamark:light:colour" v="green">
							<text class="light-character" dx="1.9" dy="0"><tag k="seamark:light:character"/>.G.<tag k="seamark:light:period"/>s</text>
						</rule>
						<rule e="node" k="seamark:light:colour" v="white">
							<text class="light-character" dx="1.9" dy="0"><tag k="seamark:light:character"/>.<tag k="seamark:light:period"/>s</text>
						</rule>
					</rule>
					<else>
						<rule e="node" k="seamark:light:colour" v="red">
							<text class="light-character" dx="1.9" dy="0"><tag k="seamark:light:character"/>.R</text>
						</rule>
						<rule e="node" k="seamark:light:colour" v="green">
							<text class="light-character" dx="1.9" dy="0"><tag k="seamark:light:character"/>.G</text>
						</rule>
						<rule e="node" k="seamark:light:colour" v="white">
							<text class="light-character" dx="1.9" dy="0"><tag k="seamark:light:character"/></text>
						</rule>
					</else>
				</else>
			</else>
		</else>
	</rule>

  <!-- Signal Stations -->
  
  <rule e="node" k="seamark:type" v="signal_station_traffic">
    <symbol ref="Signal_Station" position="center" />
    <rule e="node" k="seamark:signal_station_traffic:category" v="*">
      <text class="light-character" dx="1.2" dy="0">SS(<tag k="seamark:signal_station_traffic:category"/>)</text>
    </rule>
    <else>
      <text class="light-character" dx="1.2" dy="0">SS</text>
    </else>
    <rule e="node" k="seamark:signal_station_traffic:channel" v="*">
      <text class="light-character" dx="1.2" dy="1.4">Ch.<tag k="seamark:signal_station_traffic:channel"/></text>
    </rule>
  </rule>
  
  <rule e="node" k="seamark:type" v="signal_station_warning">
    <symbol ref="Signal_Station" position="center" />
    <rule e="node" k="seamark:signal_station_warning:category" v="*">
      <text class="light-character" dx="1.2" dy="0">SS(<tag k="seamark:signal_station_warning:category"/>)</text>
    </rule>
    <else>
      <text class="light-character" dx="1.2" dy="0">SS</text>
    </else>
    <rule e="node" k="seamark:signal_station_warning:channel" v="*">
      <text class="light-character" dx="1.2" dy="1.4">Ch.<tag k="seamark:signal_station_warning:channel"/></text>
    </rule>
  </rule>
  
	<!-- Marina -->
  
	<rule e="way" k="seamark:harbour_area:category" v="marina|yacht|yacht_harbour">
    <symbol ref="Marina" position="center" />
	</rule>
  
	<!-- Names -->

	<rule e="node" k="seamark:name" v="*">
    <rule e="node" k="seamark:type" v="light_major|light_minor">
      <text k="seamark:name" class="buoy-name" dx="0" dy="-1.5" />
    </rule>
    <else>
      <text k="seamark:name" class="buoy-name" dx="1.9" dy="-1.5" />
    </else>
	</rule>

	<!-- Traffic Separation Schemes -->
  
	<rule e="way" k="seamark:type" v="separation_zone|separation_crossing|separation_roundabout">
		<area class="separation-area" layer="-1" />
	</rule>
  
	<rule e="way" k="seamark:type" v="separation_line">
		<line class="separation-line" layer="-1" />
	</rule>
  
	<rule e="way" k="seamark:type" v="separation_boundary">
		<line class="separation-dashed" layer="-1" />
	</rule>
  
  <rule e="way" k="seamark:type" v="separation_lane">
    <line class="lane-arrow" layer="-1" />
  </rule>
  
	<!-- Leading & Transit Lines -->

	<rule e="way" k="seamark:type" v="recommended_track">
		<line class="leading-line" />
    <rule e="way" k="seamark:recommended_track:orientation" v="*">
      <pathText k="seamark:recommended_track:orientation" class="line-character" startOffset="50%" dy="-0.3" />
    </rule>
	</rule>
  
	<rule e="way" k="seamark:type" v="navigation_line">
		<line class="transit-line" />
    <rule e="way" k="seamark:navigation_line:orientation" v="*">
      <pathText k="seamark:navigation_line:orientation" class="line-character" startOffset="50%" dy="-0.3" />
    </rule>
	</rule>

	<!-- Walls -->
  
	<rule e="way" k="seamark:type" v="shoreline_construction">
    <rule e="way" k="seamark:shoreline_construction:category" v="training_wall">
      <pathText class="line-character" startOffset="50%" dy="-0.5">Training Wall</pathText>
      <rule e="way" k="seamark:shoreline_construction:water_level" v="covers">
        <line class="wall-dashed" />
        <pathText class="line-character" startOffset="50%" dy="1.5">(covers)</pathText>
      </rule>
      <else>
        <line class="wall-line" />
      </else>
    </rule>
	</rule>
  
  <rule e="node" k="seamark:fixme" v="*">
    <symbol ref="LoLcaption" position="center" />
  </rule>
  
	<defs>
		<style type="text/css" xmlns="http://www.w3.org/2000/svg">
			.map-background {
      fill: none;
      stroke: none;
			}
      
			.buoy-name {
      font-family: "Arial";
      font-weight: bold;
      font-size: 1.4px;
      text-anchor: start;
			}
      
			.light-character {
      font-family: "Arial";
      font-weight: normal;
      font-size: 1.4px;
      text-anchor: start;
			}
      
			.signal-character {
      font-family: "Arial";
      font-weight: normal;
      font-size: 1.4px;
      text-anchor: end;
			}
      
			.line-character {
      font-family: "Arial";
      font-weight: normal;
      font-size: 1.4px;
      text-anchor: middle;
			}
      
			.separation-area {
      stroke: none;
      fill: #c480ff;
      fill-opacity: 0.5;
			}
      
			.separation-line {
      stroke-width: 0.8px;
      stroke-linecap: butt;
      stroke-linejoin: round;
      fill: none;
      stroke: #c480ff;
      stroke-opacity: 0.5;
			}
      
			.separation-dashed {
      stroke-width: 0.5px;
      stroke-dasharray: 2,2;
      stroke-linecap: butt;
      stroke-linejoin: round;
      fill: none;
      stroke: #c480ff;
      stroke-opacity: 0.5;
			}
			
			.lane-arrow {
			fill: none;
			stroke: #ffffff;
			stroke-width: 0.1px;
			stroke-opacity: 0;
			marker-mid: url(#lane-arrow);
			}
      
			.leading-line {
      stroke-width: 0.2px;
      stroke-linecap: butt;
      stroke-linejoin: round;
      fill: none;
      fill-opacity: 0;
      stroke: #000000;
			}
      
			.transit-line {
      stroke-width: 0.2px;
      stroke-dasharray: 0.8,0.8;
      stroke-linecap: butt;
      stroke-linejoin: round;
      fill: none;
      fill-opacity: 0;
      stroke: #000000;
			}
			
			.wall-line {
      stroke-width: 0.3px;
      stroke-linecap: butt;
      stroke-linejoin: round;
      fill: none;
      fill-opacity: 0;
      stroke: #000000;
			}
      
			.wall-dashed {
      stroke-width: 0.3px;
      stroke-dasharray: 1,1;
      stroke-linecap: butt;
      stroke-linejoin: round;
      fill: none;
      fill-opacity: 0;
      stroke: #000000;
			}
			
		</style>
    
		<svg:marker fill="none" id="lane-arrow" markerHeight="2px" markerUnits="userSpaceOnUse" markerWidth="2px" orient="auto" refX="1px" refY="5px" stroke="#c480ff" stroke-width="1px" stroke-opacity="0.5" viewBox="0 0 2 2">
			<svg:path d="M 15,5 L 11,10 L 11,7 L 0,7 L 0,3 L 11,3 L 11,0 Z"/>
		</svg:marker>
    
	</defs>

</rules>