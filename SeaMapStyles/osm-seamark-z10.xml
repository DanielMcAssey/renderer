    </rule>
    <rule e="node" k="seamark:landmark:category" v="cairn">
      <symbol ref="Cairn" position="center" />
    </rule>
	</rule>

	<!-- Major Light -->

	<rule e="node" k="seamark:type" v="light_major">
		<symbol ref="Light_Major" position="center" />
	</rule>

	<!-- Light Vessel -->
  
	<rule e="node" k="seamark:type" v="light_vessel">
		<symbol ref="Super" position="center" />
	</rule>
  
