<?xml version='1.0' encoding='UTF-8'?>
<?xml-stylesheet type="text/xsl" href="osmarender.xsl"?>


<!-- OpenSeaMap-Sport Rules File for zoom level 11 -->

<rules 
	data="../work/data.osm"
	scale="0.291271"
	symbolScale="0.25"
	minimumMapWidth="0.5"
	minimumMapHeight="0.5"
	showScale="no"
	showGrid="no"
	showBorder="no"
	showLicense="no"
	showZoomControls="no"
	symbolsDir="../SeaMapSymbols"
	meter2pixel="0.1375">
  
	<rule e="way" k="whitewater:section_grade" v="0">
    <line class="whitewater-0" layer="-5"/>
  </rule>
  
	<rule e="way" k="whitewater:section_grade" v="1|1+|1-">
    <line class="whitewater-1" layer="-5"/>
  </rule>
  
	<rule e="way" k="whitewater:section_grade" v="2|2+|2-">
    <line class="whitewater-2" layer="-5"/>
  </rule>
  
	<rule e="way" k="whitewater:section_grade" v="3|3+|3-">
    <line class="whitewater-3" layer="-5"/>
  </rule>
  
	<rule e="way" k="whitewater:section_grade" v="4|4+|4-">
    <line class="whitewater-4" layer="-5"/>
  </rule>
  
	<rule e="way" k="whitewater:section_grade" v="5|5+|5-">
    <line class="whitewater-5" layer="-5"/>
  </rule>
  
	<rule e="way" k="whitewater:section_grade" v="6|6+|6-">
    <line class="whitewater-6" layer="-5"/>
  </rule>
  
	<defs>
		<style type="text/css" xmlns="http://www.w3.org/2000/svg">
      
			.map-background { fill: #b5d0d0; fill-opacity: 0; stroke: none; }
      
			.whitewater-0 { fill: none; stroke: #0080ff; stroke-opacity: 0.75; stroke-width: 8.0px; stroke-linecap: butt; stroke-linejoin: round; }
			.whitewater-1 { fill: none; stroke: #00d400; stroke-opacity: 0.75; stroke-width: 8.0px; stroke-linecap: butt; stroke-linejoin: round; }
			.whitewater-2 { fill: none; stroke: #ffd400; stroke-opacity: 0.75; stroke-width: 8.0px; stroke-linecap: butt; stroke-linejoin: round; }
			.whitewater-3 { fill: none; stroke: #ff8000; stroke-opacity: 0.75; stroke-width: 8.0px; stroke-linecap: butt; stroke-linejoin: round; }
			.whitewater-4 { fill: none; stroke: #ff0000; stroke-opacity: 0.75; stroke-width: 8.0px; stroke-linecap: butt; stroke-linejoin: round; }
			.whitewater-5 { fill: none; stroke: #8000ff; stroke-opacity: 0.75; stroke-width: 8.0px; stroke-linecap: butt; stroke-linejoin: round; }
			.whitewater-6 { fill: none; stroke: #a000a0; stroke-opacity: 0.75; stroke-width: 8.0px; stroke-linecap: butt; stroke-linejoin: round; }
      
		</style>
	</defs>
</rules>
