<?xml version='1.0' encoding='UTF-8'?>
<?xml-stylesheet type="text/xsl" href="osmarender.xsl"?>


<!-- OpenSeaMap-Sport Rules File for zoom level 12 -->

<rules 
	data="../work/data.osm"
	scale="0.291271"
	symbolScale="0.15"
	minimumMapWidth="0.5"
	minimumMapHeight="0.5"
	showScale="no"
	showGrid="no"
	showBorder="no"
	showLicense="no"
	showZoomControls="no"
	symbolsDir="../SeaMapSymbols"
	meter2pixel="0.1375">
  
	<rule e="way" k="sport" v="fishing|kite_surfing|canoeing|rowing|sailing|scuba_diving|surfing|swimming|water_ski|wind_surfing|water_sports">
    <area class="sport-area" layer="-5"/>
  </rule>
  
	<defs>
		<style type="text/css" xmlns="http://www.w3.org/2000/svg">
      
			.map-background { fill: #b5d0d0; fill-opacity: 0; stroke: none; }
      
			.sport-area { fill: none; stroke: #6fc18e; stroke-opacity: 0.25; stroke-width: 2.0px; }
      
		</style>
	</defs>
</rules>